{
  "name": "aperdia/aperdiaui",
  "description": "UI Basis for Laravel 5.5",
  "keywords": ["laravel", "bulma.io"],
  "license": "MIT",
  "authors": [
    {
      "name": "caouecs",
      "email": "caouecs@caouecs.net"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": "~5.5",
    "laravelcollective/html": "~5.5",
    "amstaffix/pagination": "~1.1"
  },
  "autoload": {
    "psr-0": {
      "Aperdia\\AperdiaUI": "src/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Aperdia\\AperdiaUI\\Provider\\AperdiaUIServiceProvider"
      ],
      "aliases": {
        "Alert": "Aperdia\\AperdiaUI\\Alert",
        "Breadcrumb": "Aperdia\\AperdiaUI\\Breadcrumb",
        "Form": "Aperdia\\AperdiaUI\\Form",
        "Link": "Aperdia\\AperdiaUI\\Link",
        "Pagination": "Aperdia\\AperdiaUI\\Pagination",
      }
    }
  }
}
